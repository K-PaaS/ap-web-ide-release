check process ap-web-ide-broker
  with pidfile /var/vcap/sys/run/ap-web-ide-broker/ap-web-ide-broker.pid
  start program "/var/vcap/jobs/ap-web-ide-broker/bin/monit_debugger service_ctl '/var/vcap/jobs/ap-web-ide-broker/bin/service_ctl start'"
  stop program "/var/vcap/jobs/ap-web-ide-broker/bin/monit_debugger service_ctl '/var/vcap/jobs/ap-web-ide-broker/bin/service_ctl stop'"
  group vcap
